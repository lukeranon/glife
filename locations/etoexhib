	if temp > 90:'In the depths of the attic you hear a group of <a href="exec:gt ''etoexhib'', ''pos50''">teens</a> talking. It seems they do not notice you.'

	act 'Go to the roof':
		cla
		*clr
		gs 'stat'
		gt 'etoexhib', 'pos53'
	end

	act 'Go to the hallway':gt 'etoexhib', 'pos51'
end
if $ARGS[0] = 'pos53':
	cla
	*clr
	minut += 5
	gs 'stat'
	'<center><img src="images/etogame/roof.jpg"></center>'
	'The roof of your apartment building.'

	if temper >= 20 and bikini > 0 and hour >= 6 and hour <= 20:
		act 'Sunbath (1:00)':
			cla
			*clr
			set minut += 60
			set sweat += 1
			set manna += 5
			'<center><img src="images/etogame/zagar1.jpg"></center>'

			if krem = 0:
				set tan += 1
				'You lie down to sunbathe.'
			end

			if krem > 0:
				set krem -= 1
				set tan += 3
				'You put the sunblock on your body and lied down on the roof to sunbathe.'
			end

			act 'Get Up':gt 'etoexhib', 'pos53'
		end
	end

	act 'Go to the attic':
		cls
		gt 'etoexhib', 'pos52'
	end
end

